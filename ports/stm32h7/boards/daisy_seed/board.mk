QSPI_FLASH = IS25LP064A

CFLAGS += \
  -DSTM32H750xx \
  -DHSE_VALUE=16000000U \
  -DTINYUF2_LED=1 \
  -DBOARD_QSPI_FLASH_EN=1 \
  -DBOARD_AXISRAM_EN=1 \
  -D$(QSPI_FLASH)\
  -DBOARD_FLASH_APP_START=0x90000000 \
  -DTRAP_EXC \

MCU = h750xx

LOG = 0

SRC_S += \
  $(ST_CMSIS)/Source/Templates/gcc/startup_stm32h750xx.s
